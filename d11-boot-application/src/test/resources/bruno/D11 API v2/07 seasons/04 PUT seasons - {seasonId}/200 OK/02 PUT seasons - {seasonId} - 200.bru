meta {
  name: PUT seasons/{seasonId} - 200
  type: http
  seq: 2
}

put {
  url: {{baseUrl}}/v2/seasons/{{seasonId}}
  body: json
  auth: bearer
}

auth:bearer {
  token: {{jwt}}
}

body:json {
  {
      "season": {
          "name": "1970-1971",
          "d11TeamBudget": 5,
          "d11TeamMaxTransfers": 1,
          "status": "PENDING",
          "date": "1970-01-01",
          "legacy": true
      }
  }
}

assert {
  res.status: eq 200
}

tests {
  const { expectToEq } = require('./d11.js');
  
  test("PUT season response body", () => {
    const body = res.getBody();
    const seasonId = JSON.parse(bru.getEnvVar("seasonId"));
    const season = {
      id: seasonId,
      name: "1970-1971",
      shortName: "70-71",
      d11TeamBudget: 5,
      d11TeamMaxTransfers: 1,
      status: "PENDING",
      date: "1970-01-01",
      legacy: true
   };
  
    expectToEq.season(body.season, season);
  });
  
}
