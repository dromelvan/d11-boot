meta {
  name: Setup - Update Transfer Day Status
  type: http
  seq: 4
}

put {
  url: {{baseUrl}}/v2/transfer-days/{{currentTransferDayId}}
  body: json
  auth: bearer
}

auth:bearer {
  token: {{jwt}}
}

body:json {
  {
      "transferDay": {
          "transferDayNumber": 2,
          "status": "ACTIVE",
          "datetime": "{{currentTransferDayDatetime}}"
      }
  }
}

assert {
  res.status: eq 200
}
