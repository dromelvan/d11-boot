meta {
  name: GET players/{playerId}/player-season-stats - 400 Invalid Player Id
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/v2/players/invalid/player-season-stats
  body: none
  auth: none
}

assert {
  res.status: eq 400
  res.body.error: eq Bad Request
  res.body.method: eq GET
  res.body.path: eq /v2/players/invalid/player-season-stats
  res.body.validationErrors: isDefined
  res.body.validationErrors.length: eq 1
  res.body.validationErrors[0].property: eq playerId
  res.body.validationErrors[0].error: eq is invalid
}
