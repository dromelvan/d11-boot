meta {
  name: GET players/{playerId}/player-match-stats?seasonId={seasonId} - 400 Negative Player Id
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/v2/players/-1/player-match-stats?seasonId={{seasonId}}
  body: none
  auth: none
}

params:query {
  seasonId: {{seasonId}}
}

assert {
  res.status: eq 400
  res.body.error: eq Bad Request
  res.body.method: eq GET
  res.body.path: eq /v2/players/-1/player-match-stats
  res.body.validationErrors.length: eq 1
  res.body.validationErrors[0].property: eq playerId
  res.body.validationErrors[0].error: eq must be positive
}
