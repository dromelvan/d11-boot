meta {
  name: Setup - Login Admin
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/v2/security/authenticate
  body: json
  auth: none
}

body:json {
  {
      "username": "{{adminUsername}}",
      "password": "{{adminPassword}}",
      "persistent": true
  }
  
}

assert {
  res.status: eq 200
}

script:post-response {
  const body = res.getBody();    
  bru.setEnvVar("jwt", body.jwt);
}

