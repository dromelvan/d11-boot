meta {
  name: GET d11-matches?matchWeekId={matchWeekId} - 400 Missing Match Week Id
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/v2/d11-matches
  body: none
  auth: none
}

assert {
  res.status: eq 400
  res.body.error: eq Bad Request
  res.body.method: eq GET
  res.body.path: eq /v2/d11-matches
  res.body.validationErrors.length: eq 1
  res.body.validationErrors[0].property: eq matchWeekId
  res.body.validationErrors[0].error: eq is missing
}
