meta {
  name: GET d11-teams/{d11TeamId}/d11-team-season-stats?seasonId={seasonId} - 400 Invalid Season Id
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/v2/d11-teams/{{d11TeamId}}/d11-team-season-stats?seasonId=invalid
  body: none
  auth: none
}

params:query {
  seasonId: {{seasonId}}
}

assert {
  res.status: eq 400
  res.body.error: eq Bad Request
  res.body.method: eq GET
  res.body.path: eq /v2/d11-teams/{{d11TeamId}}/d11-team-season-stats
  res.body.validationErrors.length: eq 1
  res.body.validationErrors[0].property: eq seasonId
  res.body.validationErrors[0].error: eq is invalid
}
