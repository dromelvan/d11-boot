meta {
  name: GET teams/{teamId}/team-season-stats?seasonId={seasonId} - 400 Negative Season Id
  type: http
  seq: 4
}

get {
  url: {{baseUrl}}/v2/teams/{{teamId}}/team-season-stats?seasonId=-1
  body: none
  auth: none
}

params:query {
  seasonId: {{seasonId}}
}

assert {
  res.status: eq 400
  res.body.error: eq Bad Request
  res.body.method: eq GET
  res.body.path: eq /v2/teams/{{teamId}}/team-season-stats
  res.body.validationErrors.length: eq 1
  res.body.validationErrors[0].property: eq seasonId
  res.body.validationErrors[0].error: eq must be positive
}
